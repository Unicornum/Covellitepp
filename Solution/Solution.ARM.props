<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
    <Import Project="Solution\Externals\Default.props" />
    <Import Project="Solution.props" />
    <Import Project="Solution\Externals\Covellite\Covellite.props" />
  </ImportGroup>
  <PropertyGroup Label="UserMacros">
    <PlatformToolsetVersion>0</PlatformToolsetVersion>
  </PropertyGroup>
  <PropertyGroup>
    <UseMultiToolTask>true</UseMultiToolTask>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile />
    <ClCompile>
      <AdditionalOptions>-fopenmp -Wno-nonportable-include-path -Wno-invalid-source-encoding %(AdditionalOptions)</AdditionalOptions>
      <PreprocessorDefinitions>%(PreprocessorDefinitions);__ANDROID__</PreprocessorDefinitions>
      <EnableNeonCodegen>true</EnableNeonCodegen>
    </ClCompile>
    <Link>
      <AdditionalOptions>-fopenmp %(AdditionalOptions)</AdditionalOptions>
      <LibraryDependencies>unwind;OpenSLES;%(LibraryDependencies)</LibraryDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="PlatformToolsetVersion">
      <Value>$(PlatformToolsetVersion)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>